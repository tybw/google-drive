{% extends 'base.html.twig' %}
{% block title %}Add new user{% endblock %}
{% block stylesheets %}{% endblock %}
{% block body %}
    <h1>Add new user</h1>
    {% for msg in app.session.flashbag.get('error') %}
        {{ msg }}
    {% endfor %}
    {{ form_start(form) }}
    {{ form_row(form.email) }}
    <input type="submit" name="create" value="Create new user">
    {{ form_end(form) }}
{% endblock %}
{% block javascripts %}{% endblock %}
